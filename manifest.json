{
    "manifest_version": 3,
    "name": "Adblock Hu",
    "description": "ad blocker",
    "version": "1.1",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "images/BlockLogo.png"
    },
    "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
        }]
    },
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ]
    
}